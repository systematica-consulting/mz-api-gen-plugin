                          <xs:annotation>
                            <xs:documentation>Номер журнала, в котором опубликовано решение</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="ГРНДата" type="fnst:ГРНДатаТип">
                      <xs:annotation>
